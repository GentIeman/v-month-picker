{
  "name": "v-month-picker",
  "description": "Simple year and month picker inspired by vuetify's datepicker",
  "author": "Ilya Shepelev <gentleman-2021@main.ru>",
  "version": "0.2.7",
  "private": false,
  "license": "MIT",
  "main": "./dist/monthpicker.common.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build-bundle": "vue-cli-service build --target lib --name monthpicker src/lib.js"
  },
  "dependencies": {
    "v-click-outside": "^3.1.2",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "keywords": [
    "vue",
    "month picker",
    "javascript",
    "vuejs",
    "vue.js",
    "month-picker",
    "vue component"
  ],
  "files": [
    "dist/*",
    "src/*",
    "*.json"
  ],
  "homepage": "https://github.com/GentIeman/v-month-picker#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GentIeman/v-month-picker.git"
  }
}
